        <h1>
            Codeial / <%= title %>
        </h1>

        <section id="feed-post">
        <h1>Post</h1>
        <% if (locals.user) { %>
        <form action="/posts/create" id="new-post" method="post">
        <textarea name="content" id="" cols="30" rows="3"></textarea>
        <input type="submit" value="post">
        </form>
        <% } %>
        <div id="post-container" >
        <ul>
            <% for(post of posts){%>
            <li>
                <p>
                <%= post.content %>
                <br>
                <small>
                    <%= post.user.name %>
                    
                </small>
            </p>
            </li>
                <%} %>
        </ul>
        </div>
        </section>